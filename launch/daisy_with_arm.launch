<launch>
    <group ns="daisy">
        <node pkg="hebi-ros-daisy" type="hexapod_control" name="hexapod_control" output="screen" required="true"/>
        <node pkg="hebi-ros-daisy" type="controller" name="controller" output="screen" required="true"/>
    </group>
    <group ns="arm">
        <include file="$(find hebi_cpp_api_examples)/launch/arm_node.launch">
            <arg name="arm_type" value="A-2240-06"/>
        </include>
    </group>
    <rosparam>
        families: ["Lily"]
    </rosparam>
</launch>
